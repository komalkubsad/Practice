
public class MI {
	public static void main(String[] args) {
		char fi = 'K';
		char mi = 'P';
		char li = 'K';
		
		System.out.println( + fi + mi + li); // with data type 'char' 'K', 'P', 'K' are stored with their underlying ASCI values and the results in a number 230 
		System.out.println("Initials : " + fi + mi + li);
	}

}
